    $(document).ready(function () {
    });
    function AnHien_GiaTri() {
        var arrayVM = $("#UCViewHoChuaThuyDienPublic1_ddlMien").val();
        var tmpVM = '';
        if (tmpVM != null && tmpVM != '') {
            arrayVM = tmpVM.split("-");
        }
        if (arrayVM != null) {
            $('#UCViewHoChuaThuyDienPublic1_ddlMien > option').each(function () {
                $(this).attr("selected", false);
                var itemoption = $(this).val();
                $(arrayVM).each(function (i, v) {
                    if (itemoption == v)
                        $("#UCViewHoChuaThuyDienPublic1_ddlMien > option[value=" + v + "]").attr("selected", true);
                });
            });
        }
    }

    function TuDongThayDoiURL() {
        var d = new Date(),
        month = '' + (d.getMonth() + 1),
        day = '' + d.getDate(),
        year = d.getFullYear();
        if (month.length < 2) month = '0' + month;
        if (day.length < 2) day = '0' + day;
        var getden = '';
        var getvm = '';
        var getlv = '';
        var gethc = '';

        if ($("#UCViewHoChuaThuyDienPublic1_tbxDenNgay").val() != null) {
            getden = $("#UCViewHoChuaThuyDienPublic1_tbxDenNgay").val();
        }
        if ($("#UCViewHoChuaThuyDienPublic1_ddlMien").val() != null) {
            getvm = $("#UCViewHoChuaThuyDienPublic1_ddlMien").val().toString().replace(/,/g, "-");
        }
        if ($("#UCViewHoChuaThuyDienPublic1_ddlLuuVuc").val() != null) {
            getlv = $("#UCViewHoChuaThuyDienPublic1_ddlLuuVuc").val().toString().replace(/,/g, "-");
        }
        if ($("#UCViewHoChuaThuyDienPublic1_ddlHoChua").val() != null) {
            gethc = $("#UCViewHoChuaThuyDienPublic1_ddlHoChua").val().toString().replace(/,/g, "-");
        }
        location.replace("/PageHoChuaThuyDienEmbedEVN.aspx?td=" + getden + "&vm=" + getvm + "&lv=" + getlv + "&hc=" + gethc);
    }

    function ChangeThoiDiem() {
        var geththientai = '';
        var gethtquery = '';
        if ($("#UCViewHoChuaThuyDienPublic1_tbxDenNgay").val() != null) {
            geththientai = $("#UCViewHoChuaThuyDienPublic1_tbxDenNgay").val().toString().replace(/,/g, "-");
        }
        if (geththientai != gethtquery) {
            TuDongThayDoiURL();
        }
    }

